<body>
	<table>
		<thead>
			<tr>
				<th>Username</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Role</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= user.username %></td>
					<td><%= user.firstname %></td>
					<td><%= user.lastname %></td>
					<td><%= user.role %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</body>